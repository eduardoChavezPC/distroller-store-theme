{
  "header": {
    "blocks": ["header-layout.desktop", "header-layout.mobile"]
  },
  "header.full": {
    "blocks": ["header-layout.desktop", "header-layout.mobile"]
  },
  "header-layout.desktop": {
    "children": [
      "flex-layout.row#1-desktop",
      "flex-layout.row#2-desktop", // Notification bar
      "flex-layout.row#3-desktop", // Search bar - Stores - Cart
      "sticky-layout#4-desktop"
    ]
  },

  "flex-layout.row#1-desktop": {
    "children": ["telemarketing"],
    "props": {
      "fullWidth": true
    }
  },

  "flex-layout.row#2-desktop": {
    "children": [/*"notification.bar#home"*/
      "flex-layout.col#pais",
      "flex-layout.col#redes",
      "flex-layout.col#txtNotBar",
      "flex-layout.col#newsletter"
    ],
    "props": {
      "fullWidth": "true",
      "maxHeight":"40px",
      "height": 40,
      "blockClass": "notificationBar"
    }
  },
  "notification.bar#home": {
    "props": {
      "content": "SELECTED ITEMS ON SALE! CHECK IT OUT!"
    }
  },

