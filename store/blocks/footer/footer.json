    "props":{"verticalAlign":"middle",
      "blockClass":"footer-mobile"
    },
    "children":[
      "rich-text#footer-mobile-text"
    ]
  },
  "rich-text#footer-mobile-text":{
    "props":{
      "preserveLayoutOnMobile":true,
