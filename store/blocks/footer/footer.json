{
  "footer": {
    "blocks": [
      "footer-layout.desktop",
      "footer-layout.mobile"
      ]
  },
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#footer-1-desktop",
      "flex-layout.row#footer-3-desktop"
    ]
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "vtex.menu@2.x:menu#menuVertical1",
      "vtex.menu@2.x:menu#menuVertical2",
      "flex-layout.row#imagen"
    ],
    "props": {
      "blockClass": "menu-row",
      "paddingTop": 6,
      "paddigBottom": 3
    }
  },

  "flex-layout.row#imagen":{
    "props":{
      "width":"50%"
    },
    "children":[
      "flex-layout.col#footerImagen"
    ]
  },

  "flex-layout.col#footerImagen":{
    "props":{
      "horizontalAlign":"center"
    },
    "children":[
      "image#footerImageDistroller"
    ]
  },
  "image#footerImageDistroller":{
    "props":{
      "src":"assets/distroller_white_1.svg",
      "blockClass":"imagFooter"

    }
  },

  "flex-layout.row#footer-3-desktop": {
    "children": [
      "flex-layout.row#footerRow"
    ],
    "props": {
      "blockClass": "credits"
    }
  },
  "flex-layout.row#footerRow":{
    "props":{
      "blockClass":"footerRow"  
    },
    "children":[
      "flex-layout.col#footerCol1",
      "footer-spacer#1",
      "flex-layout.col#footerCol4"
      ]
  },
  "footer-spacer#1":{
    "props":{
      "width":"7%"
    }
  },
  "flex-layout.col#footerCol1":{
    "props":{
      "horizontalAlign":"right",
      "verticalAlign":"middle"
    },
    "children":[
      "rich-text#footer"
    ]
  },
  "flex-layout.col#footerCol4":{
    "props":{
      "horizontalAlign":"left",
      "verticalAlign":"middle",
      "blockClass":"footerText"
    },
    "children":[
      "rich-text#footer4"
    ]
  },"rich-text#footer": {
    "props": {
      "horizontalAlign":"center",
      "text":"Made with <span>**&#10084;**</span> by Punto Commerce",
      "blockClass": "footer1"
    }
  },
  "rich-text#footer4": {
    "props": {
      "horizontalAlign":"center",
      "text": "Distroller 2022. Todos los derechos reservados",
      "blockClass": "footer4"
    }
  },






  "footer-layout.mobile": {
    "props":{
        "blockClass":"footer-mobile"
    },
    "children": [
      "flex-layout.row#1-footer-mobile",
      "flex-layout.row#2-footer-mobile",
      "flex-layout.row#3-footer-mobile",
      "flex-layout.row#4-footer-mobile",
      "flex-layout.row#5-footer-mobile"
    ]
  },
  "flex-layout.row#4-footer-mobile":{
    "props":{
      "verticalAlign":"middle",
      "blockClass":"imagenDistroller"
    },
    "children":[
      "image#distroller"
    ]
  },
  "image#distroller":{
    "props":{
      "src":"assets/distroller_white_1.svg"
    }
  },
  "flex-layout.row#3-footer-mobile":{
    "props":{
      "preserveLayoutOnMobile":true,
      "horizontalAlign":"center",
      "preventHorizontalStretch": true,
      "blockClass":"imagenesRedes"
    },
    "children":[
      "flex-layout.col#img7",
      "flex-layout.col#img6",
      "flex-layout.col#img5",
      "flex-layout.col#img4",
      "flex-layout.col#img3",
      "flex-layout.col#img2",
      "flex-layout.col#img1"
    ]
  },
  "flex-layout.col#img1":{
    "props":{
      "marginLeft":2,
      "marginRight":2,
      "verticalAlign":"middle",
      "blockClass":"imgRed-1"
    },
    "children":[
      "image#red-1"
    ]
  },
  "flex-layout.col#img2":{
