{
  "store.custom#faq": {
    "children": [
      "flex-layout.row#faq"
    ]
  },

  "flex-layout.row#faq": {
    "props": {
      "horizontalAlign":"center",
      "blockClass": "faq"
    },
    "children": [
      "flex-layout.col#faq"
    ]
  },

  "flex-layout.col#faq": {
    "props":{
      "horizontalAlign":"center"
    },
    "children": [
      
      "rich-text#faq",
      "disclosure-layout-group#group"
    ]
  },
  "rich-text#faq": {
    "props": {
      "blockClass":"titulofaq",
      "text": "## Preguntas frecuentes"
    }
  },

  "disclosure-layout-group#group": {
    "children": [
      "disclosure-layout#firstb",
      "flex-layout.row#linea",
      "disclosure-layout#secondb",
      "flex-layout.row#linea",
      "disclosure-layout#thirdb",
      "flex-layout.row#linea",
      "disclosure-layout#fourthb",
      "flex-layout.row#linea",
      "disclosure-layout#fifthb",
      "flex-layout.row#linea"//,
      // "flex-layout.row#fecha",
      // "flex-layout.row#boton-test"
    ]
  },
  "flex-layout.row#boton-test":{
    "props":{
      "blockClass":"boton-test-row"
    },
    "children":[
      "flex-layout.col#boton-test"
    ]
  },
  "flex-layout.col#boton-test":{
    "props":{
      "horizontalAlign":"center",
      "blockClass":"boton-test-col"
    },
    "children":[
      "rich-text#button-rt"
    ]
  },
  "rich-text#button-rt":{
    "props":{
      "blockClass":"buttontext",
      "text":"[Button Test](/button)"
    }
  },
  "flex-layout.row#fecha":{
    "props":{
      "horizontalAlign":"center",
      "blockClass":"fecha"
    },
    "children":[
      "rich-text#fecha"
    ]
  },
  "rich-text#fecha":{
    "props":{
      "blockClass":"fecha",
      "text":"Este documento fue modificado en 26/Enero/2022"
    }
  },
  "flex-layout.row#linea":{
    "props":{
      "blockClass":"linea-faq"
    }
  },
